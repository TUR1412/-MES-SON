<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
  </startup>
  <connectionStrings>
    <!-- MySQL数据库连接字符串 -->
    <add name="MESConnectionString" connectionString="Server=localhost;Port=3306;Database=mes;User Id=mes;CharSet=utf8mb4;SslMode=none;AllowPublicKeyRetrieval=true;" providerName="MySql.Data.MySqlClient" />
  </connectionStrings>
  <appSettings>
    <!-- 日志配置 -->
    <add key="LogLevel" value="Info" />
    <add key="LogPath" value="logs" />
  </appSettings>
</configuration>